{
  "name": "resmon-guiclient",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.4.1",
    "babel-plugin-import": "^1.7.0",
    "chai": "^4.1.2",
    "chai-enzyme": "^0.8.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "highcharts": "^6.1.0",
    "history": "^4.7.2",
    "moment": "^2.22.1",
    "react": "^16.3.2",
    "react-app-rewire-less": "^2.1.1",
    "react-app-rewired": "^1.5.0",
    "react-dom": "^16.3.2",
    "react-props": "^0.0.3",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "1.1.4",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "test:nowatch": "CI=true ENV=production react-app-rewired test --env=jsdom",
    "test:coverage": "CI=true react-app-rewired test --env=jsdom --coverage",
    "eject": "react-app-rewired eject"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!<rootDir>/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "coverageReporters": [
      "text"
    ]
  }
